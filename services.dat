{
  "services": [
    {
      "name": "xnode",
      "executable": "xnode.exe",
      "working_dir": "F://end//XControl-1.4.1-win//xnode",
      "args": ["run", "--env-file", "../.env"],
      "health_check": {
        "enabled": true,
        "url": "http://127.0.0.1:9860",
        "endpoint": "/",
        "max_retries": 30,
        "retry_interval_ms": 1000
      }
    },
    {
      "name": "nanomq",
      "executable": "nanomq.exe",
      "working_dir": "F://end//XControl-1.4.1-win//nanomq//bin",
      "args": ["start", "--conf", "nanomq.conf"]
    },
    {
      "name": "xnc",
      "executable": "xnc.exe",
      "working_dir": "F://end//XControl-1.4.1-win//device"
    },
    {
      "name": "xplay",
      "executable": "xplay.exe",
      "working_dir": "F://end//XControl-1.4.1-win//xplay"
    }
  ]
}